        <form class="dodajOgloszenie" action="/dodaj" method="POST">

            <div class="form-group col-md-6">
                <label for="jezyk">Language</label>
                <input class="form-control" name="jezyk" id="jezyk" placeholder="Chosen language">
            </div>

            <div class="form-group col-md-6">
                <label for="cena">Price</label>
                <input class="form-control" name="cena" id="cena" placeholder="Price per hour">
            </div>

            <div class="form-group col-md-6">
                <label for="opis">Description</label>
                <textarea class="form-control" name="opis" id="opis" rows="3"></textarea>
            </div>

            <button type="submit" class="btn btn-primary dodajPrzycisk">Add</button>
        </form>

