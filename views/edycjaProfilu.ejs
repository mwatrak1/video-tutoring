<%- include("komponenty/header") -%>
</head>
<%- include("komponenty/nawigacja") -%>
<div class="edycjaProfilu">
<form class="dodajOgloszenie" action="/edycjaProfilu" enctype="multipart/form-data" method="post" >

    <div class="form-group col-sm-6">
        <label for="jezyk_ojczysty">Język ojczysty</label>
        <input  class="form-control" name="jezyk_ojczysty" id="jezyk_ojczysty" value="<%=profil.jezyk_ojczysty%>" placeholder="Język ojczysty">
    </div>

    <div class="form-group col-sm-6">
        <label for="lokalizacja">Lokalizacja</label>
        <input  class="form-control" name="lokalizacja" id="lokalizacja" value="<%=profil.lokalizacja%>" placeholder="Podaj miejscowość">
    </div>
    <div class="form-group col-sm-6">
        <label for="znane_jezyki">Znane języki</label>
        <input class="form-control" name="znane_jezyki" id="znane_jezyki" value="<%=profil.znane_jezyki%>" placeholder="Znane języki">
    </div>
	  <div class="form-group col-sm-6">
        <label for="imie">Imie</label>
        <input class="form-control" name="imie" id="imie" value="<%=profil.imie%>" placeholder="Imie">
    </div>
	  <div class="form-group col-sm-6">
        <label for="nazwisko">Nazwisko</label>
        <input class="form-control" name="nazwisko" id="nazwisko" value="<%=profil.nazwisko%>" placeholder="Nazwisko">
    </div>
	  <div class="form-group col-sm-6">
        <label for="opis">Opis</label>
        <textarea class="form-control" name="opis" id="opis" value="<%=profil.opis%>" rows="3"></textarea>
    </div>
    <div class="form-group col-sm-6">
        <label for="zdjecie">Zdjecie profilowe</label></br>
        <input  type="file" accept=".jpg" name="zdjecie" id="zdjecie" value="<%=profil.zdjecie%>">
    </div>
    <div class="form-group col-sm-6">
        <label for="film">Filmik promujacy</label></br>
        <input  type="file" accept=".mp4"  name="film" id="film" value="<%=profil.film%>">
    </div>

   <button type="submit" class="btn btn-primary dodajPrzycisk">Dodaj</button>
</form>
</div>

<script src="./scripts/edycjaProfilu.js"></script>

<%- include("komponenty/footer") %> 